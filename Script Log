                ["inline"] = false
            },
            {            
                ["name"] = "Device:",
                ["value"] = "``` " .. Device .. " ```",
                ["inline"] = false
            },
            {
                ["name"] = "Place Name:",
                ["value"] = "``` " .. game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId).Name .. " ```",
                ["inline"] = false
